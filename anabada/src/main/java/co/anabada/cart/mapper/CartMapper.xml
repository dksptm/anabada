<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="co.anabada.mapper.CartMapper">

	<select id="cartList" resultType="cart">
		SELECT cart_id,
		member_num,
		item_num,
		cart_status 
		 FROM cart;
	</select>
	
	<insert id="insertCart" parameterType="co.anabada.cart.Cart">
		INSERT INTO cart(member_num,item_num,cart_status)
		VALUES (#{memberNum},#{itemNum},#{cartStatus})
	</insert>

	
</mapper>