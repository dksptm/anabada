  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="co.anabada.cart.CartMapper">

  <select id="cartList" resultType="co.anabada.cart.Cart">
